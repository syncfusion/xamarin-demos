<resources><declare-styleable name="SfBusyIndicator"><!--        Determine the animation types--><attr name="busyIndicatorAnimationTypes" format="enum"><enum name="ball" value="0" /><enum name="battery" value="1" /><enum name="box" value="2" /><enum name="doubleCircle" value="3" /><enum name="ecg" value="4" /><enum name="gearBox" value="5" /><enum name="globe" value="6" /><enum name="horizontalPulsingBox" value="7" /><enum name="movieTimer" value="8" /><enum name="print" value="9" /><enum name="rectangle" value="10" /><enum name="rollingBall" value="11" /><enum name="singleCircle" value="12" /><enum name="slicedCircle" value="13" /><enum name="zoomingTarget" value="14" /></attr><!--        color of animations and text--><attr name="busyIndicatorTextColor" format="color" /><!--        height and width of busyindicator view--><attr name="busyIndicatorViewBoxHeight" format="integer" value="200" /><attr name="busyIndicatorViewBoxWidth" format="integer" value="100" /><!--        title of bustindicator--><attr name="busyIndicatorTitle" format="string" /><!--        size of title in busyindicator--><attr name="busyIndicatorTextSize" format="integer" /><!--        color of secondary color in busyindicator--><attr name="busyIndicatorSecondaryColor" format="color" /><!--        title placement--><attr name="busyIndicatorTitlePlacement" format="enum"><enum name="top" value="0" /><enum name="bottom" value="1" /><enum name="none" value="2" /></attr><!--        enable or disable busyness--><attr name="busyIndicatorIsBusy" format="boolean" /></declare-styleable></resources>