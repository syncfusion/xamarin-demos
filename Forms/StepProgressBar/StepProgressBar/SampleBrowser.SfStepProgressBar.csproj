<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GenerateAssemblyVersionAttribute>false</GenerateAssemblyVersionAttribute>
    <Configurations>Debug;Release;Release-xml</Configurations>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="SamplesList\SamplesList.xml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="SamplesList\SamplesList.xml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="SampleBrowser.Core" Version="24.2.3" />
    <PackageReference Include="Syncfusion.Xamarin.Buttons" Version="24.2.3" />
    <PackageReference Include="Syncfusion.Xamarin.SfComboBox" Version="24.2.3" />
    <PackageReference Include="Syncfusion.Xamarin.Core" Version="24.2.3" />
    <PackageReference Include="Syncfusion.Xamarin.DataSource" Version="24.2.3" />
    <PackageReference Include="Syncfusion.Xamarin.GridCommon" Version="24.2.3" />
    <PackageReference Include="Syncfusion.Xamarin.SfListView" Version="24.2.3" />
    <PackageReference Include="Syncfusion.Xamarin.SfProgressBar" Version="24.2.3" />
    <PackageReference Include="Xamarin.Forms" Version="5.0.0.2012" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Samples\ShipmentTracking\ShipmentTracking.xaml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Samples\Themes\Themes.xaml.cs">
      <DependentUpon>Themes.xaml</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Samples\Themes\Themes.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\UserRegistration\UserRegistration.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
  </ItemGroup>
  <Import Project="Sdk.props" Sdk="Microsoft.NET.Sdk" />
</Project>