<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	<TargetFramework>netstandard1.0</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GenerateAssemblyVersionAttribute>false</GenerateAssemblyVersionAttribute>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Xamarin.Compression" Version="16.3.0.21" />
    <PackageReference Include="Syncfusion.Xamarin.DocIO" Version="16.3.0.21" />
    <PackageReference Include="Syncfusion.Xamarin.OfficeChart" Version="16.3.0.21" />
    <PackageReference Include="Syncfusion.Xamarin.Pdf" Version="16.3.0.21" />
    <PackageReference Include="Syncfusion.Xamarin.XlsIO" Version="16.3.0.21" />
    <PackageReference Include="Xamarin.Forms" version="2.4.0.280" />
  </ItemGroup>
  <Import Project="Sdk.props" Sdk="Microsoft.NET.Sdk" />
  <Import Project="Sdk.targets" Sdk="Microsoft.NET.Sdk" />
  <ItemGroup>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="App.xaml.cs">
      <DependentUpon>*.xaml</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="InvoiceTemplate_iOS.xlsx" />
    <EmbeddedResource Include="InvoiceTemplate.xlsx" />
    <EmbeddedResource Include="Template.docx" />
    <EmbeddedResource Include="Template_iOS.docx" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="SyncfusionLogo.jpg" />
  </ItemGroup>
</Project>